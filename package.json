{
  "name": "plone-mosaic",
  "version": "1.0.0",
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "bootstrap": "^5.0.0-alpha3",
    "bower": "^1.8.8",
    "css-loader": "^2.1.0",
    "exports-loader": "^0.6.3",
    "imports-loader": "^0.6.5",
    "less": "^3.9.0",
    "less-loader": "^2.2.3",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "plonetheme-webpack-plugin": "^2.0.1",
    "requirejs": "^2.1.20",
    "style-loader": "^0.13.1",
    "text-loader": "0.0.1",
    "url-loader": "^1.1.2",
    "watch": "^0.16.0",
    "webpack": "^4.34.0",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^0.14.0"
  },
  "dependencies": {
    "brace": "^0.10.0"
  },
  "scripts": {
    "watch": "nodemon --watch scss/ --ext scss --exec \"yarn css-main\"",
    "css-compile-grid": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 src/plone/app/mosaic/browser/static/scss/mosaic-grid.scss -o src/plone/app/mosaic/browser/static/",
    "css-compile-mosaic": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 src/plone/app/mosaic/browser/static/scss/plone-mosaic.scss -o src/plone/app/mosaic/browser/static/",
    "css-compile-styles": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 src/plone/app/mosaic/browser/static/scss/mosaic-styles.scss -o src/plone/app/mosaic/browser/static/",
    "css-main": "npm-run-all css-compile-grid css-compile-mosaic css-compile-styles"
  }
}
