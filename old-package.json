{
  "name": "plone-mosaic",
  "version": "1.0.0",
  "devDependencies": {
    "autoprefixer": "^10.0.1",
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "bootstrap": "^5.1.3",
    "bower": "^1.8.8",
    "clean-css-cli": "^4.3.0",
    "nodemon": "^2.0.14",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.1",
    "postcss-cli": "^8.1.0",
    "requirejs": "^2.1.20",
    "sass": "",
    "stylelint-config-twbs-bootstrap": "^2.1.0",
    "watch": "^0.13.0"
  },
  "dependencies": {
    "brace": "^0.10.0"
  },
  "scripts": {
    "watch": "nodemon --watch scss/ --ext scss --exec \"npm run css-main\"",
    "css-compile-grid": "sass --style expanded src/plone/app/mosaic/browser/static/scss/mosaic-grid.scss:src/plone/app/mosaic/browser/static/mosaic-grid.css",
    "css-compile-mosaic": "sass --style expanded src/plone/app/mosaic/browser/static/scss/plone-mosaic.scss:src/plone/app/mosaic/browser/static/plone-mosaic.css",
    "css-compile-styles": "sass --style expanded src/plone/app/mosaic/browser/static/scss/mosaic-styles.scss:src/plone/app/mosaic/browser/static/mosaic-styles.css",
    "css-main": "npm-run-all css-compile-grid css-compile-mosaic css-compile-styles"
  }
}
